import requests

token = '3e625bfc3e625bfc3e625bfc1d3e09536933e623e625bfc629e4872951fb2558db71494'
version = 5.52
user_id = 10205574

response = requests.get(
    'https://api.vk.com/method/friends.get',
    params={
            'user_id': user_id,
            'fields': 'nickname', 
            'access_token': token,
            'v': version 
           }
)

data = response.json()
print(data)




